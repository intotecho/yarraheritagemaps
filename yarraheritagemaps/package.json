{
  "name": "yarraheritagemaps",
  "version": "0.0.1",
  "license": "TBD",
  "scripts": {
    "start": "ng serve",
    "installcors": "gsutil.cmd cors set gcsbucket-cors-config.json  gs://historic_map_overlays",
    "builddocs": "pandoc -f markdown -t html --css=src/styles.css --self-contained --metadata pagetitle='User Guide' userdocs/userguide.md -o userdocs/userguide.html",
    "deploydocs": "gsutil.cmd cp userdocs/userguide.html gs://historic_map_overlays",
    "installserver": "./buildserver.sh",
    "server": "cd server && . Scripts/activate && python main.py",
    "ng": "ng",
    "build": "cp -r src/assets/* server/app/assets/ && ng build --prod --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "deploy": "cd server && . Scripts/activate && gcloud app deploy --quiet app.yaml",
    "compodoc": "npx compodoc -p src/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
